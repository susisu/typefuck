{
  "name": "@susisu/typefuck",
  "version": "0.5.0",
  "description": "Type-level Brainfuck interpreter in TypeScript",
  "repository": "https://github.com/susisu/typefuck.git",
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "files": [
    "lib",
    "src",
    "!src/**/*.{test,spec}.{ts,tsx}",
    "!src/**/__tests__"
  ],
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write src '*.js'",
    "format:check": "prettier --check src '*.js'",
    "lint": "eslint --fix src '*.js'",
    "lint:check": "eslint src '*.js'",
    "test": "tsc -p tsconfig.json --noEmit",
    "build": "tsup",
    "prepublishOnly": "run-s format:check lint:check test build"
  },
  "peerDependencies": {
    "typescript": ">=4.1.2"
  },
  "devDependencies": {
    "@susisu/eslint-config": "^0.0.90",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "~5.6.2"
  }
}
