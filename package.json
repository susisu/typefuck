{
  "name": "@susisu/typefuck",
  "version": "0.4.0",
  "description": "Type-level Brainfuck interpreter in TypeScript",
  "repository": "https://github.com/susisu/typefuck.git",
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "!src/**/*.{test,spec}.{ts,tsx}",
    "!src/**/__tests__"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "run-s lint:eslint lint:prettier",
    "lint-fix": "run-s lint-fix:eslint lint-fix:prettier",
    "lint:eslint": "eslint src '*.js'",
    "lint-fix:eslint": "eslint --fix src '*.js'",
    "lint:prettier": "prettier --check src '*.js'",
    "lint-fix:prettier": "prettier --write src '*.js'",
    "test": "tsc -p tsconfig.test.json --noEmit",
    "build": "tsc --build tsconfig.build.json",
    "clean": "run-s clean:build clean:rm",
    "clean:build": "tsc --build tsconfig.build.json --clean",
    "clean:rm": "rimraf lib",
    "prepublishOnly": "run-s clean lint test build"
  },
  "peerDependencies": {
    "typescript": ">=4.1.2"
  },
  "devDependencies": {
    "@susisu/eslint-config": "^0.0.67",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "globals": "^13.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  }
}
