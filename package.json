{
  "name": "@susisu/typefuck",
  "version": "0.4.0",
  "description": "Type-level Brainfuck interpreter in TypeScript",
  "repository": "https://github.com/susisu/typefuck.git",
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "!src/**/*.{test,spec}.{ts,tsx}",
    "!src/**/__tests__"
  ],
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write src '*.js'",
    "format:check": "prettier --check src '*.js'",
    "lint": "eslint --fix src '*.js'",
    "lint:check": "eslint src '*.js'",
    "test": "tsc -p tsconfig.test.json --noEmit",
    "build": "tsc --build tsconfig.build.json",
    "clean": "run-s clean:build clean:rm",
    "clean:build": "tsc --build tsconfig.build.json --clean",
    "clean:rm": "rimraf lib",
    "prepublishOnly": "run-s clean format:check lint:check test build"
  },
  "peerDependencies": {
    "typescript": ">=4.1.2"
  },
  "devDependencies": {
    "@susisu/eslint-config": "^0.0.72",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^13.23.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2"
  }
}
